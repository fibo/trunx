{
  "name": "trunx",
  "version": "0.28.0",
  "description": "Super Saiyan React components, son of awesome Bulma, implemented in TypeScript",
  "main": "index.js",
  "scripts": {
    "browserify": "cross-env NODE_ENV=production browserify src/docs/index.js -t babelify -g [ envify --NODE_ENV production ] -g uglifyify | terser --compress --mangle > docs/bundle.js",
    "build": "run-s build:*",
    "build:docs": "npm run browserify",
    "build:lib": "npm run tsc",
    "eslint": "eslint . --fix",
    "postversion": "git push origin v${npm_package_version}; npm publish; git push origin master",
    "pretsc": "rimraf component",
    "sass": "node-sass --include-path node_modules src/docs/sass/style.scss --output-style compressed -o docs/",
    "start": "budo src/docs/index.js --dir docs --serve bundle.js --open --live --pushstate -- -t babelify",
    "test": "run-p test:*",
    "test:bulma-version": "node test/bulma-version.js",
    "test:types": "tsc --declaration --project . --noemit",
    "tsc": "tsc --declaration --project ."
  },
  "pre-commit": [
    "eslint",
    "test"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/fibo/trunx.git"
  },
  "keywords": [
    "react",
    "bulma",
    "component",
    "components",
    "typescript"
  ],
  "author": {
    "name": "Gianluca Casati",
    "url": "http://g14n.info"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/fibo/trunx/issues"
  },
  "homepage": "http://trunx.dev",
  "peerDependencies": {
    "react": "16.x"
  },
  "devDependencies": {
    "@types/react": "^16.9.17",
    "@types/react-dom": "^16.9.4",
    "@typescript-eslint/eslint-plugin": "^2.13.0",
    "@typescript-eslint/parser": "^2.13.0",
    "babel-eslint": "^10.0.2",
    "dot-editorconfig": "^1.1.1",
    "eslint": "^6.8.0",
    "eslint-config-standard": "^14.1.0",
    "eslint-config-standard-jsx": "^8.1.0",
    "eslint-plugin-import": "^2.19.1",
    "eslint-plugin-node": "^10.0.0",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-react": "^7.17.0",
    "eslint-plugin-react-hooks": "^2.3.0",
    "eslint-plugin-standard": "^4.0.1",
    "fa-svg-icon": "^0.3.0",
    "node-sass": "^4.13.0",
    "npm-run-all": "^4.1.2",
    "pre-commit": "^1.2.2",
    "react-router-dom": "^5.0.1",
    "react-syntax-highlighter": "^8.0.1",
    "rimraf": "^2.6.3",
    "typescript": "^3.7.4",
    "zeroconf-redux": "^5.3.0"
  },
  "dependencies": {
    "bulma": "^0.8.0",
    "classnames": "^2.2.6",
    "tslib": "^1.10.0"
  }
}
