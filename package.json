{
  "name": "trunx-monorepo",
  "version": "0.0.0",
  "type": "module",
  "private": true,
  "scripts": {
    "build": "npm run build --workspaces --if-present",
    "cleanup": "npm run cleanup --workspaces --if-present",
    "check_types": "npm run check_types --workspaces --if-present",
    "generate_BulmaClassName_type": "postcss ./node_modules/bulma/css/bulma.css --config ./npm-scripts/generate_BulmaClassName_type --output ./node_modules/bulma/css/output.css",
    "lint": "npm run lint --workspaces --if-present",
    "prestart": "npm run build -w packages/trunx",
    "start": "npm run dev -w webapps/trunx-docs",
    "test": "npm run test --workspaces --if-present"
  },
  "devDependencies": {
    "@types/node": "^18.11.18",
    "@types/react": "^18.0.27",
    "@types/react-dom": "^18.0.10",
    "@typescript-eslint/eslint-plugin": "^5.51.0",
    "@typescript-eslint/parser": "^5.51.0",
    "eslint": "^8.34.0",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-react": "^7.32.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "pre-commit": "^1.2.2",
    "prettier": "^2.7.1",
    "postcss": "^8.4.21",
    "postcss-cli": "^10.1.0",
    "postcss-ts-classnames": "^0.3.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "rimraf": "^4.1.2",
    "typescript": "^4.9.4"
  },
  "pre-commit": [
    "check_types",
    "lint",
    "test"
  ],
  "workspaces": [
    "packages/trunx",
    "webapps/trunx-docs"
  ],
  "dependencies": {}
}
