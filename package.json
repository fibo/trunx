  "description": "super Saiyan react components, son of awesome Bulma",
  "main": "index.js",
  "scripts": {
    "babel": "babel --ignore stories.js src -d .",
    "deploy-storybook": "storybook-to-ghpages",
    "lint": "run-p lint:*",
    "lint:src-js": "cd src && standard --fix",
    "lint:src-ts": "tslint -c tslint.json 'src/**/*.{ts,tsx}'",
    "lint:storybook": "cd .storybook && standard --fix",
    "postversion": "git push origin v${npm_package_version}; npm publish; git push origin master",
    "typescript": "tsc --project .",
    "storybook": "start-storybook -p 9001 -c .storybook",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "pre-commit": [
    "lint"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/fibo/trunx.git"
  },
  "keywords": [
    "react",
    "bulma",
    "component",
    "components"
  ],
  "author": {
    "name": "Gianluca Casati",
    "url": "http://g14n.info"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/fibo/trunx/issues"
  },
  "homepage": "http://g14n.info/trunx",
  "peerDependencies": {
    "react": "16.x"
  },
  "devDependencies": {
    "@storybook/addon-options": "^3.4.7",
    "@storybook/react": "4.0.0-alpha.3",
    "@storybook/storybook-deployer": "^2.3.0",
    "@types/react": "^16.4.1",
    "@types/react-dom": "^16.0.6",
    "awesome-typescript-loader": "^5.1.1",
    "babel-core": "^6.26.3",
    "bulma": "^0.7.1",
    "dot-editorconfig": "^1.1.0",
    "node-sass": "^4.9.0",
    "npm-run-all": "^4.1.2",
    "pre-commit": "^1.2.2",
    "react": "^16.4.1",
    "react-dom": "^16.4.1",
    "tslint": "^5.10.0",
    "tslint-react": "^3.6.0",
    "typescript": "^2.9.2"
  },
