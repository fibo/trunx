{
  "name": "trunx",
  "version": "0.18.4",
  "description": "Super Saiyan React components, son of awesome Bulma, implemented in TypeScript",
  "main": "component/index.js",
  "scripts": {
    "deploy-storybook": "storybook-to-ghpages",
    "lint": "run-p lint:*",
    "lint:src-js": "cd src && standard",
    "lint:src-ts": "tslint -c tslint.json 'src/**/*.{ts,tsx}'",
    "lint:storybook": "cd .storybook && standard",
    "postversion": "git push origin v${npm_package_version}; npm publish; git push origin master",
    "pretypescript": "rimraf component",
    "test": "npm run typescript--noemit",
    "typescript": "tsc --declaration --project .",
    "typescript--noemit": "tsc --declaration --project . --noemit",
    "storybook": "start-storybook -p 9001 -c .storybook"
  },
  "pre-commit": [
    "lint",
    "test"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/fibo/trunx.git"
  },
  "keywords": [
    "react",
    "bulma",
    "component",
    "components",
    "typescript"
  ],
  "author": {
    "name": "Gianluca Casati",
    "url": "http://g14n.info"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/fibo/trunx/issues"
  },
  "homepage": "http://g14n.info/trunx",
  "peerDependencies": {
    "react": "16.x"
  },
  "devDependencies": {
    "@babel/core": "^7.2.2",
    "@storybook/addon-options": "^4.1.7",
    "@storybook/addon-viewport": "^4.1.7",
    "@storybook/react": "^4.1.7",
    "@storybook/storybook-deployer": "^2.3.0",
    "@types/react": "^16.8.2",
    "@types/react-dom": "^16.8.0",
    "awesome-typescript-loader": "^5.2.1",
    "babel-loader": "^8.0.4",
    "bulma": "^0.7.4",
    "dot-editorconfig": "^1.1.1",
    "fa-svg-icon": "^0.3.0",
    "node-sass": "^4.9.4",
    "npm-run-all": "^4.1.2",
    "pre-commit": "^1.2.2",
    "react": "^16.8.1",
    "react-dom": "^16.8.1",
    "react-syntax-highlighter": "^8.0.1",
    "rimraf": "^2.6.2",
    "standard": "^12.0.1",
    "tslint": "^5.12.1",
    "tslint-react": "^3.6.0",
    "typescript": "^3.3.3"
  },
  "dependencies": {
    "classnames": "^2.2.6",
    "tslib": "^1.9.3"
  }
}
